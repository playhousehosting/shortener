{
  "name": "shortener",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist --kv=SHORTENER_LINKS --d1=shortener_database --local --persist --live-reload --port 3000",
    "wrangler": "pnpm run build && pnpm run preview",
    "nodemon": "nodemon --watch 'src/**/*' --ext astro,tsx,ts --exec pnpm run build"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^6.2.4",
    "@astrojs/image": "^0.16.7",
    "@astrojs/prefetch": "^0.2.1",
    "@astrojs/sitemap": "^1.3.0",
    "@astrojs/tailwind": "^3.1.2",
    "astro": "^2.4.2",
    "astro-compress": "^1.1.35",
    "astro-google-fonts-optimizer": "^0.2.2",
    "astro-icon": "^0.8.0",
    "astro-robots-txt": "^0.4.1",
    "jose": "^4.14.0",
    "nanoid": "^4.0.2",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "query-string": "^8.1.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230404.0",
    "autoprefixer": "^10.4.14",
    "better-sqlite3": "^8.3.0",
    "cssnano": "^6.0.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.8.0",
    "wrangler": "^2.15.0"
  }
}
